includes:
    - vendor/enterprise-tooling-for-symfony/shared-bundle/config/phpstan.app.dist.neon

parameters:
    level: 10
    paths:
        - bin/
        - config/
        - public/
        - src/
        - tests/
        - migrations/
    excludePaths:
        analyse:
            - tests/Architecture/*
    type_coverage:
        return: 100
        param: 100
        property: 100
        constant: 100
    ergebnis:
        final:
            enabled: false
        noExtends:
            enabled: false
        noNullableReturnTypeDeclaration:
            enabled: false
        noConstructorParameterWithDefaultValue:
            enabled: false
        noParameterWithNullDefaultValue:
            enabled: false
        noParameterWithNullableTypeDeclaration:
            enabled: false
